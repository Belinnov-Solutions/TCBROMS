
@{
    Layout = null;
}
<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
<script src="https://code.jquery.com/jquery-3.4.1.min.js"
        integrity="sha256-CSXorXvZcTkaix6Yvo6HppcZGetbYMGWSFlBw8HfCJo="
        crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>
<link href="https://cdnjs.cloudflare.com/ajax/libs/mdbootstrap/4.15.0/css/mdb.min.css" rel="stylesheet">
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>uplodProductExcel</title>
</head>
<body>
    <div>
        <div class="row">
            <div class="col-md-6">
                <div class="form-group">
                    <input type="hidden" id="getname" />
                    <label>File Upload</label>
                    <div class="input-group">
                        <input id="ExcelUpload" type="file">
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="box-footer">
                <button type="button" id="btnUploads" class="btn btn-primary" tabindex="5">Upload</button>
                @*<button type="button" id="btnCancel" class="btn btn-primary" tabindex="6" onclick="ResetAll()">Cancel</button>*@
                <button id="buttonmodal" type="button" data-toggle="modal" class="btn btn-primary" data-target="#UploadRecordsList" style="display: none; float: right;" title="You must try and do things in the correct order to make sure everyone">Open order</button>
            </div>

        </div>
    </div>
    <script>

    $(function () {

        $("#btnUploads").click(function () {
                var formData = new FormData();
                var totalFiles = document.getElementById("ExcelUpload").files.length;
                if (totalFiles != 0) {
                    for (var i = 0; i < totalFiles; i++) {
                        var file = document.getElementById("ExcelUpload").files[i];
                        formData.append("ExcelUpload", file);
                    }

                    $.ajax(
                        {
                            type: "POST",
                            //url: "../Order/GetCreateOrderData",
                            //url: "../Order/UploadBarCodeData",
                            url: "@Url.Action("uplodProductExcel","Menu")",
                            data: formData,
                            dataType: 'json',
                            contentType: false,
                            processData: false,
                            success: function (response) {

                            },
                        });
                }

        });

    });


    </script>
</body>

</html>
